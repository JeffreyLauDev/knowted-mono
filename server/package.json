{
  "name": "knowted-backend",
  "version": "1.0.0",
  "description": "Backend for Knowted",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "start:prod:env:watch": "NODE_ENV=production nest start --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "prepare": "husky install",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:create": "npm run typeorm -- migration:create",
    "stripe:local:listen": "stripe listen --forward-to localhost:3000/api/v1/stripe/webhook",
    "killport3000": "lsof -ti:3000 | xargs kill -9"
  },
  "dependencies": {
    "@langchain/core": "^0.3.58",
    "@langchain/openai": "^0.5.13",
    "@meeting-baas/sdk": "^5.0.1",
    "@nestjs/common": "^10.4.18",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.18",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.4.18",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^10.4.18",
    "@opensearch-project/opensearch": "^3.5.1",
    "@supabase/supabase-js": "^2.49.8",
    "body-parser": "^2.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.1.0",
    "form-data": "3.0.1",
    "langchain": "^0.3.36",
    "mailgun.js": "12.0.3",
    "marked": "4.3.0",
    "nestjs-pino": "^4.4.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.0",
    "pino-http": "^10.5.0",
    "pino-opensearch": "^1.1.0",
    "reflect-metadata": "^0.1.13",
    "retell-sdk": "^1.15.21",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "stripe": "^18.4.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.7.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@tanstack/react-query": "^5.80.6",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.19.4",
    "@types/passport-jwt": "^4.0.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "axios": "^1.9.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^15.2.0",
    "orval": "^7.9.0",
    "pino-pretty": "^13.1.1",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.25",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.json"
        }
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!**/*.spec.ts",
      "!**/*.interface.ts",
      "!**/*.dto.ts",
      "!**/*.entity.ts",
      "!**/main.ts",
      "!**/migrations/**",
      "!**/seeds/**"
    ],
    "coverageDirectory": "../coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html",
      "json-summary"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 22,
        "functions": 20,
        "lines": 25,
        "statements": 25
      }
    },
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(marked)/)"
    ],
    "testTimeout": 10000,
    "verbose": true,
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.ts"
    ]
  },
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --write",
      "jest --findRelatedTests --passWithNoTests"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
